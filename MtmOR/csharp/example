if [ $? -ne 0 ] ; then
echo Error while compiling \(see ../log.txt for more info\)
exit 1
fi
cd ..
pushd _tmp > /dev/null
mono ./_demo.exe
popd > /dev/null
;;
*)
echo unknown compiler
exit 1
;;
esac
